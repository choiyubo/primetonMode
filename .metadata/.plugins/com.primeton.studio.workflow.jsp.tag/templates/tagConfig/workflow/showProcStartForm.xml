<?xml version="1.0" encoding="UTF-8"?>
<tag xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  tagName="wf:showProcStartForm">
	<require>
		<path>/workflow/baseWorkflow.xml</path>
		<include>id,scope</include>
	</require>
	<group groupName="基本属性">
		<property type="primitive:string" name="processID" displayName="processID" hint="流程定义ID，支持@开头变量" required="true"/>
		<property type="primitive:string" name="prefix" displayName="prefix" hint="生成的表单字段的name属性前缀" required="false" />
	</group>
</tag>



