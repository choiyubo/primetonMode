<?xml version="1.0" encoding="UTF-8"?>
<tag xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" tagName="baseWorkflow">
	<group groupName="基本属性">
		<property type="combo:readonly" name="scope" displayName="scope"  hint="读取数据的数据区" required="false" >
			<dataProvider class="listProvider">
				<string>page</string>
				<string>request</string>
				<string>flow</string>
				<string>session</string>
			</dataProvider>
		</property>
		<property name="id" displayName="id" hint="控件唯一标识" type="primitive:string" required="false" showInViewModel="never"/>
	</group>
</tag>

