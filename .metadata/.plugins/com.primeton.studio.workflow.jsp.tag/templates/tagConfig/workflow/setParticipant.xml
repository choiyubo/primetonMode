<?xml version="1.0" encoding="UTF-8"?>
<tag xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" tagName="wf:setParticipant">
	<require>
		<path>/workflow/baseWFSelect.xml</path>
		<include>id,name,value,maxNum,genScript,scope,styleClass</include>
	</require>
	<group groupName="基本属性">
		<property type="primitive:string" name="workItemID" displayName="workItemID" hint="工作项ID，支持@开头的变量" required="true"/>
		<property type="primitive:string" name="processID" displayName="processID" hint="流程定义ID，支持@开头的变量" required="false"/>
		<property type="primitive:string" name="activityID" displayName="activityID" hint="活动定定义ID，支持@开头的变量" required="false"/>
	</group>
</tag>


