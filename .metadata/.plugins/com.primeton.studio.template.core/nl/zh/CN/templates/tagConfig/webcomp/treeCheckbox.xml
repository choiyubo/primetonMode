<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<tag xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" tagName="w:treeCheckbox" >
    <require>
        <path>/webcomp/baseWebcomp.xml</path>
        <include>value,scope</include>
    </require>
    <group groupName="基本属性">
        <property displayName="field" hint="当前节点类型中和checkbox实体建立关联的字段" name="field" required="false" type="primitive:string"/>
        <property displayName="checkedXpath" hint="相关联的描述checkbox实体的名称" name="checkedXpath" required="false" type="primitive:string"/>
        <property displayName="checkedField" hint="checkbox实体中和当前节点建立关联的字段" name="checkedField" required="false" type="primitive:string"/>
    </group>
</tag>
