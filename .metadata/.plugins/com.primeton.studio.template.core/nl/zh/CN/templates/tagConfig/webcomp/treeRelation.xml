<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<tag xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" tagName="w:treeRelation" >
    <require>
        <path>/webcomp/baseWebcomp.xml</path>
        <include>value,scope</include>
    </require>
    <group groupName="基本属性">
        <property displayName="field" hint="前节点类型中和父节点类型建立关联的字段,多字段用逗号隔开" name="field" required="false" type="primitive:string"/>
        <property displayName="parentField" hint="父节点类型中和当前节点类型建立关联的字段, 多字段用逗号隔开" name="parentField" required="false" type="primitive:string"/>
        <property displayName="parentNodeType" hint="当前节点类型的父节点类型的名称" name="parentNodeType" required="true" type="primitive:string"/>
    </group>
</tag>
